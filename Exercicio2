//2. Fazer um programa que imprima a média aritmética dos números 8,9 e 7. A média dos 
//números 4, 5 e 6. A soma das duas médias. A média das médias.


package expace;

public class Exercicio2 {

	public static void main(String[] args) {
		
		 int media1 = (7 + 8 + 9)/3;
		 int media2 = (4 + 5 + 6)/3;
		 int SomaMedia = media1 + media2;
		 int MediaFinal = SomaMedia/2;

		 		 
		 System.out.println("A média aritmética dos números 8, 9 e 7 é: " +media1);
		 System.out.println("A média dos números 4, 5 e 6 é: " +media2);
		 System.out.println("A soma das duas médias é igual á: " +SomaMedia);
		 System.out.println("A média das duas médias é igual á: " +MediaFinal);
		

	}

}
